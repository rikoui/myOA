	function del_tr(obj) {
		$(obj).parent().parent().remove();
		$(":input[name='no']").each(function (i, e) {
			var id = $(e).attr("id");
			$("#" + id).val(i + 1);
		});
	}
	
	function addTable() {
		//var elements = $("#nextplan").children().length; //表示id为“nextplan”的标签下的子标签的个数
		var i = $("#count").val();
		var count = $("#nextplan").find("tr").length;
		var tableHtml = "";
		tableHtml += '<tr id="tr">'
				+ '<th align="left" style="width: 50px; height: 40px;"><input name="no" id="no' + i + '" style="width: 50px; height: 20px;" readonly="readonly"></th>'
				+ '<th align="left" style="width: 550px; height: 40px;"><input name="nextplanthing" id="nextplanthing' + i + '" style="width: 550px; height: 20px;"></th>'
				+ '<th align="left" style="width: 200px; height: 40px;">'
				+ '<input name="nextplanovertime" type="datetime" id="nextplanovertime' + i + '" onfocus="WdatePicker({dateFmt:\'yyyy-MM-dd\'})" style="width: 200px; height: 20px;">'
				+ '<th align="left" style="width: 200px; height: 40px;"><input name="nextplanleading" id="nextplanleading' + i + '" style="width: 200px; height: 20px;"></th>'
				+ '<th align="left" style="width: 200px; height: 40px;"><input name="nextplanparting" id="nextplanparting' + i + '" style="width: 200px; height: 20px;"></th>'
				+ '<th><input type="button" value="删除" id="deleteTable' + i + '" onclick="del_tr(this)"></th></tr>';
		$("#nextplan").find("tr").eq(count-1).after(tableHtml); //在表头之后添加空白行
		$("#no" + i).val(count);
		$("#count").val(i + 1);
	}
	
	// 页面加载查询
	function queryList() {
		//获取周报信息
		DMTRequest({
			url : '../../weeklyController/getWeekly.do',
			validate : true,
			success : function(data) {
				var weekly = data.result;
				$("#rolename").html(weekly.rolename);
				$("#weeklyadmin").html(weekly.weeklyadmin);
				$("#weeklystart").html(weekly.timeStart);
				$("#weeklyend").html(weekly.timeEnd);
				document.getElementById("authadmin").value =weekly.authAdmin;
				$("#lastdesc").html(weekly.lastDesc);

				var dailypart = weekly.dailyPart;
				if (!isNull(dailypart)) {
					for (var i = 0; i < dailypart.length; i++) {
						var content = "";
						var a = "dailypart";
						var j = i + 1;
						var b = a + j;
						content = dailypart[i];
						document.getElementById(b).value = content;
					}
				}

				var dailycontent = weekly.dailyContent;
				if (!isNull(dailycontent)) {
					for (var i = 0; i < dailycontent.length; i++) {
						var content = "";
						var a = "dailycontent";
						var j = i + 1;
						var b = a + j;
						content = dailycontent[i];
						document.getElementById(b).innerHTML = content;
					}
				}
				
				var dailyquestion = weekly.dailyQuestion;
				if (!isNull(dailyquestion)) {
					for (var i = 0; i < dailyquestion.length; i++) {
						var content = "";
						var a = "dailyquestion";
						var j = i + 1;
						var b = a + j;
						content = dailyquestion[i];
						document.getElementById(b).innerHTML = content;
					}
				}
				
				var dailyanswer = weekly.dailyAnswer;
				if (!isNull(dailyquestion)) {
